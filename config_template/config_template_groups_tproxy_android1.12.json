{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "[::]:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "国外出口",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      { 
		"type": "udp",
        "tag": "proxyDns",
        "server": "1.1.1.1",
        "server_port": 53,
        "detour": "国外出口"
      },
      {
		"type": "udp",
        "tag": "localDns",
        "server": "223.5.5.5",
        "server_port": 53,
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "rule_set": "geosite-category-ads-all",
        "action": "reject",
		"method": "default", 
		"no_drop": false
      },
      {
	    "action": "route", 
        "outbound": "any",
        "server": "localDns",
        "disable_cache": true
      },
      {
		"action": "route", 
        "rule_set": "geosite-cn",
        "server": "localDns"
      },
      {
		"action": "route", 
        "clash_mode": "direct",
        "server": "localDns"
      },
      { 
		"action": "route", 
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
		"action": "route", 
        "rule_set": "geosite-geolocation-!cn",
        "server": "proxyDns"
      }
    ],
    "final": "proxyDns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
          {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "::",
            "listen_port": 7080
        },
        {
            "type": "tproxy",
            "tag": "tproxy-in",
            "listen": "::",
            "listen_port": 1536
        }
  ],
  "outbounds": [
    {
      "tag":"国外出口",
      "type":"selector",
      "outbounds":[
      "国外urltest",
        "direct",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["港|台|新加|日|美|韩|泰"]}]
    },
{
      "tag":"国内出口",
      "type":"selector",
      "outbounds":[
        "direct",
        "国内urltest",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["移动|联通|电信"]}]
    },
    
{
      "tag":"国内urltest",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["移动|联通|电信"]}],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
{
      "tag":"游戏出口",
      "type":"selector",
      "outbounds":[
        "direct",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["移动|联通|电信"]}]
    },
{
      "tag":"其他地区",
      "type":"selector",
      "outbounds":[
        "direct",
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["港|台|新加|日|美|移动|联通|电信|韩|泰|最新|回国|以下"]}
    ]},
{
      "tag":"国外urltest",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["港|台|新加|日|美|韩|泰"]}],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
   
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
